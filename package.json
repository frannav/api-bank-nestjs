{
  "name": "api-bank-nestjs",
  "version": "1.0.0",
  "description": "api-bank-nestjs",
  "author": "frannav",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frannav/api-bank-nestjs.git"
  },
  "keywords": [
    "typescript",
    "nestjs",
    "template"
  ],
  "engines": {
    "node": ">=22.x",
    "pnpm": ">=9.x"
  },
  "packageManager": "pnpm@9.14.2",
  "main": "dist/main.js",
  "scripts": {
    "build": "node --run build:clean && nest build --path tsconfig.prod.json",
    "start": "node dist/main.js",
    "dev": "nest start --watch --debug=0.0.0.0:9229",
    "test": "rimraf coverage .nyc_output && concurrently 'node --run test:unit' 'node --run test:e2e' && node --run calculate-global-test-coverage",
    "test:unit": "vitest run --coverage --config vitest.config.unit.ts",
    "calculate-global-test-coverage": "tsx scripts/calculate-global-test-coverage.ts",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.mjs,cjs,.ts,.mts",
    "lint:fix": "eslint --ignore-path .gitignore . --ext .js,.mjs,cjs,.ts,.mts --fix",
    "lint:file": "eslint --ignore-path .gitignore",
    "build:clean": "rimraf dist; exit 0"
  },
  "dependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/platform-fastify": "^10.4.6",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.6",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.42",
    "@types/bcrypt": "^5.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.8.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-istanbul": "1.3.1",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-vitest": "^0.4.1",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.10",
    "nock": "^13.5.5",
    "nyc": "^17.1.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "unplugin-swc": "^1.5.1",
    "vite": "^5.4.10",
    "vitest": "^1.3.1",
    "vitest-mock-extended": "^1.3.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.3",
    "@swc/core-linux-arm64-musl": "^1.7.42",
    "@swc/core-linux-x64-gnu": "^1.7.42",
    "@swc/core-linux-x64-musl": "^1.7.42"
  }
}
